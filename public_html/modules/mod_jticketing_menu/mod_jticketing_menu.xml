<?xml version="1.0" encoding="utf-8"?>
<extension method="upgrade" type="module" client="site" version="3.0">
	<name>Jticketing Menu</name>
	<author>Techjoomla</author>
	<creationDate>26th Jul 2017</creationDate>
	<version>2.0.4</version>
	<copyright>Techjoomla. All Rights Reserved.</copyright>
	<authorEmail>extensions@techjoomla.com</authorEmail>
	<authorUrl>http://www.techjoomla.com</authorUrl>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<description>Module to shows Jticketing Menus in events</description>
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.mod_jticketing_menu.ini</language>
	</languages>
	<files>
		<filename module="mod_jticketing_menu">mod_jticketing_menu.php</filename>
		<folder>tmpl</folder>
		<folder>css</folder>
	</files>
	<!-- parameters for Joomla 1.5.x -->
	<params><!--<param name="moduleclass_sfx" type="text" default="" label="MOD_SUFIX" description="MOD_SUFIX_DES" /> --></params>
	<!-- Fields for Joomla 1.6.x and onwards -->
	<config name="params">
		<fields name="params">
			<fieldset name="basic"><field name="moduleclass_sfx" type="text" default="" label="MOD_SUFIX" description="MOD_SUFIX_DES" /></fieldset>
		</fields>
	</config>
</extension>
